        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#PhaseIntegration"/>
    </owl:Class>
    <owl:Class rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#Concepteur">
        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#Acteur"/>
    </owl:Class>
    <owl:Class rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#Conception">
        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#ClasseurAgentN"/>
    </owl:Class>
