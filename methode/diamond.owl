    <Di:ComportementIndividuel rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#FicheComportementIndividuel">
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheDiagrammeCasDUtilisation"/>
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheDocumentationActeur"/>
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheDocumentationCasUtilisation"/>
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheIdentificationEntiteActivePassive"/>
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheModeArret"/>
        <Di:document_necessaire rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#FicheModeDefaillant"/>
