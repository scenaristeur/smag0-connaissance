    </owl:Class>
    <owl:Class rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#ContraintesTechniques">
        <s:comment xml:lang="fr">une fiche des contraintes techniques qui permet d’insister sur des points particuliers tels que la sécurité.</s:comment>
        <s:label xml:lang="fr">Contraintes Techniques</s:label>
        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#ApprochePreliminaire"/>
    </owl:Class>
    <owl:Class rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#ContraintesTechnologiques">
        <s:comment xml:lang="fr">une fiche des contraintes technologiques pour le cas où le client impose certains matériels existants ou émet des préférences.</s:comment>
        <s:label xml:lang="fr">Contraintes Technologiques</s:label>
        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#ApprochePreliminaire"/>
