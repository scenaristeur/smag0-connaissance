        <s:subClassOf rdf:resource="http://smag0.blogspot.fr/ontologies/Diamond#PhaseIndividuelle"/>
    </owl:Class>
    <owl:Class rdf:about="http://smag0.blogspot.fr/ontologies/Diamond#ComportementSocial">
